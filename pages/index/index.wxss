/**index.wxss**/
/*头部样式  */
.header{
width: 274rpx;
height: 100rpx;
margin-left: 243rpx;
/* background-color: red; */
}
.header .left{
  width: 104rpx;
  height: 100%;
  float: left;

}
.header .left>image{
  width: 100%;
  height: 100%;
}
.header .right{
  float: left;
  width: 171rpx;
  height: 100%;
}
.header .right>text{
  line-height: 100rpx;
  margin-left: 30rpx; 
   font: 0.75rem/1.3rem/微软雅黑;
}

/*按钮样式  */
 .btn{
  margin-top: 60rpx;
  margin-left: 102rpx;
  width: 100%;
  height: 70rpx;
  /* background-color: red; */
} 
/*按钮边框取消  */
.btn>button{
  width: 168rpx;
  height:100%;
  text-align: center;
  line-height: 70rpx;
  float: left;
  margin-left: 20rpx;
   border: none; 
   /* background-color: yellow; */
   font-size: 25rpx;
   font: 0.85rem/1.34rem/微软雅黑;
}
/*设置选中边框显色变宽  */
 .btn .select {
  border: 1px solid #41a0fc ;
} 


